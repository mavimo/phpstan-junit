{
	"name": "mavimo/phpstan-junit",
	"description": "PHPStan JUnit error reporter",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"require": {
		"php": "~7.1",
		"phpstan/phpstan": "^0.11"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.0"
	},
	"autoload": {
		"psr-4": {"Mavimo\\PHPStan\\ErrorFormatter\\": "src/"}
	},
    "autoload-dev": {
        "classmap": ["tests", "vendor/phpstan/phpstan/tests/PHPStan"]
    }
}
